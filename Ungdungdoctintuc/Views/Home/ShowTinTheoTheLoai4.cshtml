@model IEnumerable<Ungdungdoctintuc.Models.Tin>




@foreach (var item in Model)
{
    <div class="col-sm-6">
        <div class="row">
            <div class="col-sm-5" style="float: left;">
                <a href="#">
                    <img src="~/img/img-news.png" class="img-news-part3"
                         alt="tintuc">
                </a>
            </div>
            <div class="col-sm-7 px-4" style="float: left;">
                <a href="#">
                    <h5>@item.TieuDe</h5>
                </a>

                <p>
                    @item.TomTat
                </p>
            </div>
        </div>
    </div>
}


